@using Microsoft.AspNet.Identity
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*@Styles.Render("~/Content/css")*@
    @Scripts.Render("~/bundles/modernizr")

    <meta content="Educação Financeira, Sucesso, Investimento, App, Tecnologia, Sabedoria" name="description" />
    <meta content="5 Gold - Educação &amp; Controle Financeiro" property="og:title" />
    <meta content="Educação Financeira, Sucesso, Investimento, App, Tecnologia, Sabedoria" property="og:description" />
    <meta content="summary" name="twitter:card" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="Webflow" name="generator" />

    <link href=~/Content/StiloSite.css rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Oswald:200,300,400,500,600,700%7CRoboto:100,100italic,300,300italic,regular,italic,500,500italic,700,700italic,900,900italic%7CLora:regular,italic,700,700italic%7CNoto+Serif:regular,italic">

    @*<link href="https://uploads-ssl.webflow.com/5b889b66b9decafdf8542925/css/5gold.webflow.dade0b070.css" rel="stylesheet" type="text/css" />*@
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.4.7/webfont.js" type="text/javascript"></script>
    @*<script type="text/javascript">WebFont.load({ google: { families: ["Oswald:200,300,400,500,600,700", "Roboto:100,100italic,300,300italic,regular,italic,500,500italic,700,700italic,900,900italic", "Lora:regular,italic,700,700italic", "Noto Serif:regular,italic"] } });</script>*@<!--[if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js" type="text/javascript"></script><![endif]-->
    <script type="text/javascript">!function (o, c) { var n = c.documentElement, t = " w-mod-"; n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch") }(window, document);</script>
    @*<link href="https://uploads-ssl.webflow.com/5b889b66b9decafdf8542925/5ba41c788f1f99fa0b79b64b_logotipo%205gold%20-%20Copia.png" rel="shortcut icon" type="image/x-icon" />
        <link href="https://uploads-ssl.webflow.com/5b889b66b9decafdf8542925/5ba41c7fbd50079bc345011e_logotipo%205gold%20(2).png" rel="apple-touch-icon" />*@

</head>
<body>
    <div data-collapse="medium" data-animation="default" data-duration="400" data-w-id="e6e7d113-7e4e-cb6c-0eb2-92305d854194" class="navbar w-nav">
        <div>
            <a href="/" class="brand w-nav-brand w--current"><img src="https://uploads-ssl.webflow.com/5b889b66b9decafdf8542925/5b895ba05dbd74a343676ce5_logo%2016px.png" width="118" alt="" class="image-8" /></a>
            <nav role="navigation" class="nav-menu w-nav-menu">
                @if (Request.IsAuthenticated)
                {
                    using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
                    {
                        @Html.AntiForgeryToken()
                        <a href="/" class="navlink w-nav-link">Início</a>
                        <a href="/Dashboard/Index" class="navlink w-nav-link">Dashboard</a>
                        <a href="/Conhecimento/Index" class="navlink w-nav-link">Conhecimento</a>
                        @*<a href="#" data-w-id="e6e7d113-7e4e-cb6c-0eb2-92305d8541c3" class="navlink search1 w-nav-link"></a>*@

                        if (User.Identity.Name == "fivegoldfinance@hotmail.com")
                        {
                            @Html.ActionLink("Bem Vindo " + FiveGold.Models.Mapeamento.Usuario.Get.Nome + "!", "PainelADM", "Dashboard", routeValues: null, htmlAttributes: new { @class = "navlink red w-nav-link" })
                        }
                        else
                        {
                            @*@Html.ActionLink("Bem Vindo " + FiveGold.Models.Mapeamento.Usuario.Get.Nome + "!", "Index", "Manage", routeValues: null, htmlAttributes: new { @class = "navlink red w-nav-link" })*@
                            @Html.ActionLink("Bem Vindo " + FiveGold.Models.Mapeamento.Usuario.Get.Nome + "!", "", "", routeValues: null, htmlAttributes: new { @class = "navlink red w-nav-link" })
                        }

                        <a href="javascript:document.getElementById('logoutForm').submit()" class="navlink w-nav-link">Sair</a> @*<a href="/Login/Logout" class="navlink w-nav-link">Sair</a>*@
                    }
                }
                else
                {
                    <a href="/" class="navlink w-nav-link">Início</a>
                    <a href="/Conhecimento/Index" class="navlink w-nav-link">Conhecimento</a>
                    <div data-hover="1" data-delay="0" data-w-id="e6e7d113-7e4e-cb6c-0eb2-92305d85419b" class="dropdown w-dropdown">
                        <a href="/Account/Login" class="navlink red w-nav-link">Login</a><a href="/Account/Register" class="navlink red w-nav-link">Cadastre-se</a>
                    </div>
                }
            </nav>
            <div class="menu-button w-nav-button">
                <div class="burger-menu w-icon-nav-menu"></div>
            </div>
        </div>
        <div data-w-id="0627a574-8586-5c36-072a-e8a30b5df4db" class="search-container">
            <a href="#" data-w-id="3a85243d-a401-7b81-be93-0d5d53d5c6e0" class="close-search w-inline-block">
                <img src="https://uploads-ssl.webflow.com/5b889b66b9decafdf8542925/5b889b66b9deca6fd7542a1e_5a8f48d7e8e8640001de7eac_boton-cerrar.svg" width="15" alt="" />
            </a>
            <div class="content-wrapper w-container">
                <h3 class="search-title">Pesquisa</h3>
                <form action="/search" class="search w-form">
                    <input type="search" name="query" placeholder="Faça sua busca..." maxlength="256" required="" class="search-input w-input" />
                    <input type="submit" value="Buscar" class="search-button w-button" />
                </form>
                <div class="text-mini white"></div>
            </div>
        </div>
    </div>
    <div>
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script src="https://code.jquery.com/jquery-3.3.1.min.js" type="text/javascript" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
    <script src="~/Scripts/webflow.657e11bc9.js" type="text/javascript"></script>

</body>
</html>
