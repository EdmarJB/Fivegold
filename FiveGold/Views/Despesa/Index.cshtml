@model IEnumerable<FiveGold.Models.Mapeamento.Despesa>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LoginPartial.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CatDespesa.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Conta.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Valor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descricao)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Pago)
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CatDespesa.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Conta.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Valor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Data)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descricao)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Pago)
        </td>*@
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDDespesa }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDDespesa }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDDespesa })
        </td>
    </tr>
}

</table>
